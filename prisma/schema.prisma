generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Inquiry {
  id             Int     @id @default(autoincrement())
  email          String  @unique
  name           String
  salary         String?
  essentials     String?
  non_essentials String?
  savings        String?
}
